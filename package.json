{
  "name": "apidae-bootstrap",
  "version": "0.1",
  "private": true,
  "description": "Th√®me Bootstrap pour les briques Apidae Tourisme",
  "author": "Apidae Tourisme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apidae-tourisme/ApidaeBootstrap.git"
  },
  "bugs": {
    "url": "https://github.com/apidae-tourisme/ApidaeBootstrap/issues"
  },
  "homepage": "https://github.com/apidae-tourisme/ApidaeBootstrap/#readme",
  "scripts": {
    "build": "npm-run-all --parallel css js",
    "css-compile-code": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o dist/",
    "css-compile": "sass --load-path=node_modules scss/apidae.scss dist/apidae.css",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace dist/apidae.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css dist/apidae.css --content dist/index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output dist/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3000",
    "start": "npm-run-all --parallel watch server",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "test": "npm run css-lint && npm run css",
    "js": "npm-run-all js-compile",
    "js-compile": "npx webpack --config webpack.config.js",
    "assets": "cp -r assets/* dist/"
  },
  "keywords": [
    "bootstrap",
    "apidae",
    "apidae-tourisme",
    "template",
    "theme"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.9.3",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.5.0",
    "jquery": "^3.6.0",
    "sass": "^1.38.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "move-assets-webpack-plugin": "^1.3.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^14.1.2",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  }
}